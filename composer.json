{
    "name": "softspring/symfonic",
    "description": "A powerful Symfony CMS",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "softspring/cms-bundle": "^5.3",
        "softspring/cms-translation-plugin": "^5.3",
        "softspring/cms-module-collection": "^5.3",
        "symfony/http-kernel": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.64.* || 3.66.*",
        "phpstan/phpstan": "^1.10.49 || ^2.0.0"
